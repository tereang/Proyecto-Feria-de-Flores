<!DOCTYPE	html>		
<html>			
<head>			
	<title></title>	
</head>			
<body>			
	<form method = 'GET'	action ='/'>	
	  <button class="btn btn-primary" type = 'submit'	 style = "border radius:  10px" name = 'boton_consulta' data-bs-toggle="offcanvas" data-bs-target="#offcanvasExample" aria-controls="offcanvasExample">Consulta</button>	
	</form>		
			
	{{#if ramos}}		
		<table>			
			<thead>			
				<tr>			
					<th>ID</th>			
					<th>Nombre</th>			
					<th>Stock</th>			
					<th>Precio</th>			
				</tr>			
			</thead>			
			<tbody>			
				{{#each	ramos}}		
					<tr>			
						<td>{{this.id}}</td>			
						<td>{{this.nombre}}</td>			
						<td>{{this.stock}}</td>			
						<td>{{this.precio}}</td>			
					</tr>			
				{{/each}}			
			</tbody>			
		</table>		
	{{else}}			
		<button class="btn btn-primary" type="button" style = "border radius: 10px" data-bs-toggle="offcanvas" data-bs-target="#offcanvasExample" aria-controls="offcanvasExample" ><p>No	se	encontraron	resultados</p></button>
	{{/if}}			
</body>			
</html>			
