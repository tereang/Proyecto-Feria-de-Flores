<!DOCTYPE	html>		
<html>			
<head>			
	<title></title>	
</head>			
<body>			
	<form method = 'GET'	action ='/'>	
	<button type = 'submit'	name = 'boton_consulta'>Consulta</button>	
	</form>		
			
	{{#if ramos}}		
		<table>			
			<thead>			
				<tr>			
					<th>ID</th>			
					<th>Nombre</th>			
					<th>Stock</th>			
					<th>Precio</th>			
				</tr>			
			</thead>			
			<tbody>			
				{{#each	ramos}}		
					<tr>			
						<td>{{this.id}}</td>			
						<td>{{this.nombre}}</td>			
						<td>{{this.stock}}</td>			
						<td>{{this.precio}}</td>			
					</tr>			
				{{/each}}			
			</tbody>			
		</table>		
	{{else}}			
		<p>No	se	encontraron	resultados</p>
	{{/if}}			
</body>			
</html>			
